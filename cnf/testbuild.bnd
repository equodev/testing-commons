-plugin.TestingCore: \
    aQute.bnd.repository.osgi.OSGiRepository; \
        name = "Equo Testing Core"; \
        locations = "https://dl.equo.dev/testing-core/VERSIONPLACEHOLDER/repo/index.xml.gz"; \
        poll.time = -1; \
        cache = ~/.bnd/cache/equo-testing

-plugin.TestingCoreExt: \
    aQute.bnd.repository.osgi.OSGiRepository; \
        name = "Equo Testing Core External Deps"; \
        locations = "https://dl.equo.dev/testing-core-ext/VERSIONPLACEHOLDER/repo/index.xml.gz"; \
        poll.time = -1; \
        cache = ~/.bnd/cache/equo-testing-ext

