-include ${build}/testbundle.bnd

Bundle-Version: ${version}.${tstamp}
Export-Package: \
    com.make.equo.testing.common.components,\
    com.make.equo.testing.common.statements,\
    com.make.equo.testing.common.util

-buildpath.graphics: ${if;${matches;${tolower;${os.name}};.*win.*};org.eclipse.swt.win32.win32.x86_64;${if;${matches;${tolower;${os.name}};.*mac.*};org.eclipse.swt.cocoa.macosx.x86_64;org.eclipse.swt.gtk.linux.x86_64}}

-buildpath: \
	org.eclipse.swt,\
    org.eclipse.e4.ui.workbench,\
    org.eclipse.e4.ui.workbench.swt,\
    org.eclipse.e4.ui.model.workbench,\
    org.eclipse.e4.ui.di,\
    org.eclipse.e4.core.services,\
    org.eclipse.e4.core.commands,\
    org.eclipse.core.commands,\
    org.mockito.mockito-all,\
    org.eclipse.e4.ui.workbench.renderers.swt,\
    org.apache.servicemix.bundles.junit;version=4.12