-buildpath.test: \
    osgi.cmpn,\
    org.eclipse.osgi,\
    ${junit},\
    assertj-core;version=3.15.0,\
    org.eclipse.e4.core.contexts,\
    javax.inject,\
    org.eclipse.e4.core.di,\

-runproperties: \
    osgi.console=

-testunresolved: true

-javaagent: true

-runvm.coverage: -javaagent:"${repo;org.jacoco:org.jacoco.agent:jar:runtime;0.8.5}=destfile=generated/jacoco/test.exec"

-runblacklist: \
    bnd.identity;id='org.eclipse.rap.fileupload'

-runbundles.test: \
    assertj-core;version="[3.15.0,4.0.0)",\
    org.junit; version='[4.12.0,5.0.0)',\
    org.hamcrest.core;version='[1.3.0,1.3.1)'

-runbundles.equo: \
    ch.qos.logback.classic;version='[1.1.2,1.1.3)',\
    ch.qos.logback.core;version='[1.1.2,1.1.3)',\
    ch.qos.logback.slf4j;version='[1.1.2,1.1.3)',\
    io.netty.all;version='[4.0.44,4.0.45)',\
    javax.servlet-api;version='[3.1.0,3.1.1)',\
    org.apache.commons.fileupload;version='[1.3.2,1.3.3)',\
    org.apache.commons.io;version='[2.6.0,2.6.1)',\
    org.apache.commons.logging;version='[1.2.0,1.2.1)',\
    org.apache.xmlgraphics;version='[2.3.0,2.3.1)',\
    org.eclipse.osgi.services;version='[3.8.0,3.8.1)',\
    org.eclipse.osgi.util;version='[3.5.300,3.5.301)',\
    org.eclipse.swt.chromium;version='[69.2.0,69.2.1)',\
    slf4j.api;version='[1.7.25,1.7.26)'
        
-runbundles.graphics: ${if;${matches;${tolower;${os.name}};.*win.*};org.eclipse.swt.win32.win32.x86_64;${if;${matches;${tolower;${os.name}};.*mac.*};org.eclipse.swt.cocoa.macosx.x86_64;org.eclipse.swt.gtk.linux.x86_64}}

-runbundles.console: \
    org.eclipse.equinox.console,\
    org.apache.felix.gogo.command,\
    org.apache.felix.gogo.runtime,\
    org.apache.felix.gogo.shell